<Window x:Class="dogegenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dogegenerator"
        mc:Ignorable="d"
        Title="Doge Generator" Height="1080" Width="1320" Background="DeepSkyBlue">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="The Bestest Doge" FontSize="36" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="5" Foreground="White"/>
            <ComboBox Name="dropdown" Width="250" Margin="0, 0, 0, 25" SelectionChanged="dropdown_SelectionChanged"/>
            <Canvas Height="890" Name="canvas" VerticalAlignment="Top" HorizontalAlignment="Center" Width="1220" Background="Black" ClipToBounds="True" />
        </StackPanel>
    </Grid>
</Window>
